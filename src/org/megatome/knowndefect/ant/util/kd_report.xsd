<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="knowndefects" type="knowndefectsType" />
  <xs:complexType name="methodType" mixed="true">
    <xs:attribute name="name" type="xs:string" use="required" />
  </xs:complexType>
  <xs:complexType name="kdMethodType">
    <xs:complexContent mixed="true">
      <xs:extension base="methodType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="value" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="kadMethodType">
    <xs:complexContent mixed="true">
      <xs:extension base="methodType">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="author" type="xs:string" />
          <xs:element minOccurs="1" maxOccurs="1" name="reason" type="xs:string" />
          <xs:element minOccurs="1" maxOccurs="1" name="date" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="classType">
    <xs:attribute name="name" type="xs:string" use="required" />
  </xs:complexType>
  <xs:complexType name="kdClassType">
    <xs:complexContent mixed="false">
      <xs:extension base="classType">
        <xs:sequence>
          <xs:element minOccurs="1" name="method" type="kdMethodType" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="kadClassType">
    <xs:complexContent mixed="false">
      <xs:extension base="classType">
        <xs:sequence>
          <xs:element minOccurs="1" name="method" type="kadMethodType" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="knowndefectsType">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="knowndefect">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="class" type="kdClassType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="knownandaccepteddefect">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="class" type="kadClassType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:schema>